/**app.wxss**/

view,text,swiper,swiper-item{
  box-sizing: border-box;
}
/* 在线字体代码 */
@font-face {
  font-family: 'iconfont';  /* project id 1299898 */
  src: url('//at.alicdn.com/t/font_1299898_vq1ye6hwpqr.eot');
  src: url('//at.alicdn.com/t/font_1299898_vq1ye6hwpqr.eot?#iefix') format('embedded-opentype'),
  url('//at.alicdn.com/t/font_1299898_vq1ye6hwpqr.woff2') format('woff2'),
  url('//at.alicdn.com/t/font_1299898_vq1ye6hwpqr.woff') format('woff'),
  url('//at.alicdn.com/t/font_1299898_vq1ye6hwpqr.ttf') format('truetype'),
  url('//at.alicdn.com/t/font_1299898_vq1ye6hwpqr.svg#iconfont') format('svg');
}

/* 字体文件中的代码 */
.iconfont {
  font-family: "iconfont" !important;
  font-size: 16px;
  font-style: normal;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
}

.icon-custom-comment:before {
  content: "\e61d";
}

.icon-bofangqizanting:before {
  content: "\e758";
}